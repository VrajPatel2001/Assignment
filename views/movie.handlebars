<div class="list">
    <h3 class="heading">
        Tv shows
    </h3>

<ul id="light-slider">
    
    {{#each movies}}
    <li>
        <div class="box">
    <div class="Img"><a href="/description/{{this.id}}"><img src="{{this.image}}"> </a></div>
    <div class="Text"> {{this.name}}</div>
    </div>
     </li>
    {{/each}}
   
</ul>   
</div>


